<h1>{{title}}</h1>

{{!-- clientside TensorFlow; used in nn.js--}}
<script src = "https://cdn.jsdelivr.net/npm/@tensorflow/tfjs@0.10.0";
document.getElementsByTagName('head')[0].appendChild(script)></script>
{{!-- neural net js file --}}
<script src="js/nn.js" type="text/javascript"></script>
<!-- <script src="js/input.js" type="text/javascript"></script> -->
<script src="js/login.js" type="text/javascript"></script>
<script src="js/advancedOptions.js" type="text/javascript"></script>

{{!-- jQuery --}}
<script type="text/javascript"
        src="//ajax.googleapis.com/ajax/libs/jquery/1.4.1/jquery.min.js">
</script>

<div class="body">
    <div id="greet">
        Welcome, {{name}}!
        <button type="submit" id="login" onclick="user_login()">login</button>
        <br>
    </div>

    <script src="js/jsgl.js" type="text/javascript"></script>
    <div id="panel" style ="width: 440px; height: 440px; position: absolute"></div>
    <script src="js/pentograph.js" type="text/javascript"></script>


    <div id="genre">
	Genre: 
	<input type="text" name="genre" value="genre">
	<br><br>
    </div>


    <button onclick="toggleAdvanced()">Advanced Options</button>

    <div id="advanced">
	<div class="popularity">
		<p0 style="position: relative">Popularity</p0>
		<input type="range" min="0" max="100" value="50" class="slider" id="popularity" style="position: relative">
		<br>
	</div>

	<div class="liveness">
		<p01 style="position: relative">Liveness</p01>
		<input type="range" min="0" max="100" value="50" class="slider" id="popularity" style="position: relative">
		<br>
	</div>


	<div class="key">
		<p1 style="position: relative">Key Signature</p1>
		<select name="key" style="position: relative">
			<option value="NULL"> </option>
			<option value=0>C</option>
			<option value=1>C#/Db</option>
			<option value=2>D</option>
			<option value=3>D#/Eb</option>
			<option value=4>E</option>
			<option value=5>F</option>
			<option value=6>F#/Gb</option>
			<option value=7>G</option>
			<option value=8>G#/Ab</option>
			<option value=9>A</option>
			<option value=10>A#/Bb</option>
			<option value=11>B</option>
			<option value=12>B#/Cb</option>
		</select>
		<br>
	</div>
	
	<div class="mode">
		<p2 style="position: relative">Modality</p2>
		<select name="mode" style="position: relative;left: 35px">
			<option value="NULL"> </option>
			<option value=0>Minor</option>
			<option value=1>Major</option>
		</select>
		<br>
	</div>
    </div>
   
    <script type="text/javascript">document.getElementById("advanced").style.display = "none";</script>
    <button type="submit" id="submit" onclick="submit()">Submit</button>

    </div>
    <div id="demo"></div>
    <div id="output"></div>
</div>
